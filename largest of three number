import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        
        int A = scan.nextInt();
        int B = scan.nextInt();
        int C = scan.nextInt();
        
        if (A>=B&&A>=C) {
            System.out.println(A);
        } 
        else if (B>=A&&B>=C) {
            System.out.println(B);
        } 
        else {
            System.out.println(C);
        }
        
       
    }
}
