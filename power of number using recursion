import java.io.*;
import java.util.*;

public class Solution {
    static int  pow(int a,int b)
    {
        int num=1;
        for(int i=1;i<=b;i++)
        {
           num=num*a;
    }
        return num;
    }
    public static void main(String[] args) {
       
        Scanner scan=new Scanner(System.in);
        int a=scan.nextInt();
        int b=scan.nextInt();
        System.out.println(pow(a,b));
        
        
      
    }
}
