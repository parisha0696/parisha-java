import java.util.*;
public class Solution {
    public static void main(String[] args)
    {
        Scanner scan=new Scanner(System.in);
        char ch=scan.next().charAt(0);
        switch(Character.toLowerCase(ch))
        {
            case 'a':
            case 'e':
            case 'i':
            case 'o':
            case 'u':
                System.out.println("Vowel");
                break;
            default:
                System.out.println("Consonant");
        }
    }
}
