import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int b=scan.nextInt();
        int fact=1;
        for(int i=1;i<=b;i++)
        {
            fact=fact*i;
        }
        System.out.println(fact);
    }
}
