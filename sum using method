import java.io.*;
import java.util.*;

public class Solution {
    public int add(int a,int b)
    {
      int  c=a+b;
        return c;
    }

    public static void main(String[] args) {
        Solution ob=new Solution();
      Scanner scan=new Scanner(System.in);
        int a=scan.nextInt();
        int b=scan.nextInt();
        int e=ob.add(a,b);
        System.out.println(e);
        
    }
}
