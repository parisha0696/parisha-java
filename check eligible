
import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        
        int age=scan.nextInt();
        if(age>=18)
        {
            System.out.println("Eligible");
        }
        else 
        {
            System.out.println("Not Eligible");
        }
    }
}
