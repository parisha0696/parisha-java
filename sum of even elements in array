import java.io.*;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner scan = new Scanner(System.in);

        int N = scan.nextInt();

        int sum = 0;

        for (int i = 0; i < N; i++) {

            int num = scan.nextInt();

            if (num%2== 0) {

                sum+= num;

            }

        }

        System.out.println(sum);
    }

}
