import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();      
        int a[] = new int[n];        
        int s = 0;                   

        for(int i=0;i<n;i++) {
            a[i] = scan.nextInt();   
            s = s+a[i];            
        }

        System.out.print(s);         
    }
}
