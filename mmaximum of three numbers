import java.io.*;
import java.util.*;

public class Solution {
    static int largest(int a,int b, int c)
    {
        int max=a;
        if(b>max)
        {
            max=b;
        }
        if(c>max)
        {
            max=c;
        }
        return max;
    }

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        Solution ob=new Solution();
        int a=scan.nextInt();
        int b=scan.nextInt();
        int c=scan.nextInt();
        int d=ob.largest(a,b,c);
        
        System.out.println(d);
       
    }
}
