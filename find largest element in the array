import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int n= scan.nextInt();        
        int max =0;   

        for(int i=0;i<n;i++) {
            int num = scan.nextInt();
            if(num>max) {
                max=num;             
            }
        }

        System.out.print(max);         
    }
}
