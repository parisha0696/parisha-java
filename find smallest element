import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int N = scan.nextInt();           
        int min =Integer.MAX_VALUE;     

        for(int i = 0; i < N; i++) {
            int num = scan.nextInt();     
            if(num < min) {
                min = num;                
            }
        }

        System.out.print(min);           
    }
}
