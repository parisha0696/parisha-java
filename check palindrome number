import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int N = scan.nextInt();       
        int original = N;             
        int reversed = 0;

        
        while(N > 0) {
            int digit = N % 10;
            reversed = reversed * 10 + digit;
            N = N / 10;
        }

        
        if(reversed == original) {
            System.out.print("Palindrome");
        } else {
            System.out.print("Not Palindrome");
        }
    }
}
